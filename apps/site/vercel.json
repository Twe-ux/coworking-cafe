{
  "crons": [
    {
      "path": "/api/cron/send-reminders",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/daily-report",
      "schedule": "0 19 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.coworkingcafe.fr"
        }
      ],
      "destination": "https://coworkingcafe.fr/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "new.coworkingcafe.fr"
        }
      ],
      "destination": "https://coworkingcafe.fr/:path*",
      "permanent": true
    }
  ]
}
