{
  "buildCommand": "cd ../.. && pnpm --filter @coworking-cafe/site build",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/send-reminders",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/check-attendance",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/daily-report",
      "schedule": "0 19 * * *"
    }
  ]
}
